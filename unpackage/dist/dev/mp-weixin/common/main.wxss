	/***************关键帧动画***************/
	/*放大缩小淡出*/
.fadeShow{-webkit-animation:fadeShow .4s forwards;animation:fadeShow .4s forwards;}
@-webkit-keyframes fadeShow{
0%{-webkit-transform:scale(.1);transform:scale(.1);opacity:0;}
90%{-webkit-transform:scale(1.1);transform:scale(1.1);opacity:1;}
100%{-webkit-transform:scale(1);transform:scale(1);opacity:1;}
}
@keyframes fadeShow{
0%{-webkit-transform:scale(.1);transform:scale(.1);opacity:0;}
90%{-webkit-transform:scale(1.1);transform:scale(1.1);opacity:1;}
100%{-webkit-transform:scale(1);transform:scale(1);opacity:1;}
}
	/*旋转放大淡出*/
.rs-fd{-webkit-animation:rs-fade .7s;animation:rs-fade .7s;}
@-webkit-keyframes rs-fade {
0%{-webkit-transform:rotate(20deg) scale(.5);transform:rotate(20deg) scale(.5);opacity:0;}
100%{-webkit-transform:rotate(0deg) scale(1);transform:rotate(0deg) scale(1);opacity:1;}
}
@keyframes rs-fade {
0%{-webkit-transform:rotate(20deg) scale(.5);transform:rotate(20deg) scale(.5);opacity:0;}
100%{-webkit-transform:rotate(0deg) scale(1);transform:rotate(0deg) scale(1);opacity:1;}
}
	/*放大淡出*/
.sc-fd{-webkit-animation:sc-fade .7s;animation:sc-fade .7s;}
@-webkit-keyframes sc-fade {
0%{-webkit-transform:scale(.5);transform:scale(.5);opacity:0;}
100%{-webkit-transform:scale(1);transform:scale(1);opacity:1;}
}
@keyframes sc-fade {
0%{-webkit-transform:scale(.5);transform:scale(.5);opacity:0;}
100%{-webkit-transform:scale(1);transform:scale(1);opacity:1;}
}
	/*旋转放大淡出*/
.rs-fd{-webkit-animation:rs-fade .7s;animation:rs-fade .7s;}
@keyframes rs-fade {
0%{-webkit-transform:rotate(20deg) scale(.5);transform:rotate(20deg) scale(.5);opacity:0;}
100%{-webkit-transform:rotate(0deg) scale(1);transform:rotate(0deg) scale(1);opacity:1;}
}
	/*上下晃动*/
.wobble{-webkit-animation:wobble 2s infinite alternate;animation:wobble 2s infinite alternate;}
@-webkit-keyframes wobble {
0%{-webkit-transform:translateY(-.1rem) rotateY(5deg);transform:translateY(-.1rem) rotateY(5deg);}
100%{-webkit-transform:translateY(.1rem) rotateY(-5deg);transform:translateY(.1rem) rotateY(-5deg);}
}
@keyframes wobble {
0%{-webkit-transform:translateY(-.1rem) rotateY(5deg);transform:translateY(-.1rem) rotateY(5deg);}
100%{-webkit-transform:translateY(.1rem) rotateY(-5deg);transform:translateY(.1rem) rotateY(-5deg);}
}
	/*从下往上弹出*/
.bottom-top{-webkit-animation:bottom-top .3s forwards;animation:bottom-top .3s forwards;}
@-webkit-keyframes bottom-top {
0%{-webkit-transform:translateY(100%);transform:translateY(100%);}
100%{-webkit-transform:translateY(0);transform:translateY(0);}
}
@keyframes bottom-top {
0%{-webkit-transform:translateY(100%);transform:translateY(100%);}
100%{-webkit-transform:translateY(0);transform:translateY(0);}
}
	/*从下往上淡出*/
.bt-fd{-webkit-animation:bt-fade .7s;animation:bt-fade .7s;}
@-webkit-keyframes bt-fade {
0%{-webkit-transform:translateY(40%);transform:translateY(40%);opacity:0;}
100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1;}
}
@keyframes bt-fade {
0%{-webkit-transform:translateY(40%);transform:translateY(40%);opacity:0;}
100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1;}
}
	/*从上往下淡出*/
.tb-fd{-webkit-animation:tb-fade .7s;animation:tb-fade .7s;}
@-webkit-keyframes tb-fade {
0%{-webkit-transform:translateY(-40%);transform:translateY(-40%);opacity:0;}
100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1;}
}
@keyframes tb-fade {
0%{-webkit-transform:translateY(-40%);transform:translateY(-40%);opacity:0;}
100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1;}
}
	/*从左往右向淡出*/
.lr-fd{-webkit-animation:lr-fade .7s;animation:lr-fade .7s;}
@-webkit-keyframes lr-fade {
0%{-webkit-transform:translateX(-40%);transform:translateX(-40%);opacity:0;}
100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1;}
}
@keyframes lr-fade {
0%{-webkit-transform:translateX(-40%);transform:translateX(-40%);opacity:0;}
100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1;}
}
	/*从右往左淡出*/
.rl-fd{-webkit-animation:rl-fade .7s;animation:rl-fade .7s;}
@-webkit-keyframes rl-fade {
0%{-webkit-transform:translateX(40%);transform:translateX(40%);opacity:0;}
100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1;}
}
@keyframes rl-fade {
0%{-webkit-transform:translateX(40%);transform:translateX(40%);opacity:0;}
100%{-webkit-transform:translateY(0);transform:translateY(0);opacity:1;}
}
	/*放大波浪*/
.scale-wave{-webkit-animation:scale-wave .7s infinite;animation:scale-wave .7s infinite;}
@-webkit-keyframes scale-wave {
0%{-webkit-transform:scale(1);transform:scale(1);opacity: 1;}
100%{-webkit-transform:scale(3);transform:scale(3);opacity: 0;}
}
@keyframes scale-wave {
0%{-webkit-transform:scale(1);transform:scale(1);opacity: 1;}
100%{-webkit-transform:scale(3);transform:scale(3);opacity: 0;}
}
